.page-header
  %h1
    Import

.row
  .span14
    %ul
      %li
        != link_to 'Create a new estimate', new_story_path      
      %li
        != link_to 'Import stories', new_import_path

    %h2 Import Stories from CSV file
    - form_tag import_path, :method => :post, :multipart => true do
      %label(for='file') File to Upload:
      != file_field_tag 'file'
      != submit_tag 'Import'